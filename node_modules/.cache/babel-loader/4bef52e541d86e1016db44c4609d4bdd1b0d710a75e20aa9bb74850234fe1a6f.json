{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"font-awesome/css/font-awesome.min.css\";\nimport \"animate.css\";\nimport axios from \"axios\";\nexport default {\n  name: 'App',\n  data() {\n    return {\n      save: false,\n      userId: '',\n      title: '',\n      body: '',\n      response: [],\n      loader: false\n    };\n  },\n  methods: {\n    async post(e) {\n      e.preventDefault();\n      this.loader = true;\n      try {\n        const response = await axios({\n          method: \"post\",\n          url: \"https://jsonplaceholder.typicode.com/posts/\",\n          data: {\n            userId: this.userId,\n            title: this.title,\n            body: this.body\n          }\n        });\n        this.loader = false;\n        this.response.push(response.data);\n      } catch (err) {\n        this.loader = false;\n        alert('Something is not right');\n      }\n    },\n    async deleteClick(data, index) {\n      this.loader = true;\n      let string = JSON.stringify(data);\n      let deleteData = JSON.parse(string);\n      let id = deleteData.id;\n      try {\n        await axios({\n          method: \"delete\",\n          url: \"https://jsonplaceholder.typicode.com/posts/\" + id\n        });\n        this.loader = false;\n        this.response.splice(index, 1);\n      } catch (err) {\n        this.loader = false;\n        alert('Something is not right');\n      }\n    },\n    edit(data) {\n      this.save = true;\n      let string = JSON.stringify(data);\n      let editData = JSON.parse(string);\n      this.userId = editData.userId;\n      this.title = editData.title;\n      this.body = editData.body;\n    },\n    reset() {},\n    async update() {}\n  }\n};","map":{"version":3,"names":["axios","name","data","save","userId","title","body","response","loader","methods","post","e","preventDefault","method","url","push","err","alert","deleteClick","index","string","JSON","stringify","deleteData","parse","id","splice","edit","editData","reset","update"],"sources":["D:\\VueJS\\my-app\\src\\App.vue"],"sourcesContent":["<script>\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"font-awesome/css/font-awesome.min.css\";\nimport \"animate.css\";\nimport axios from \"axios\";\n\nexport default {\n  name: 'App',\n  data(){\n    return{\n      save: false,\n      userId:'',\n      title:'',\n      body:'',\n      response: [],\n      loader: false\n    }\n  },\n  methods: {\n    async post(e){\n      e.preventDefault();\n      this.loader = true;\n      try {\n        const response = await axios({\n        method:\"post\",\n        url:\"https://jsonplaceholder.typicode.com/posts/\",\n        data: {\n          userId:this.userId,\n          title: this.title,\n          body: this.body\n        }\n      });\n      this.loader= false;\n      this.response.push(response.data);\n      }\n      catch(err){\n        this.loader= false;\n        alert('Something is not right');\n      }\n    },\n    async deleteClick(data,index){\n      this.loader = true;\n      let string = JSON.stringify(data);\n      let deleteData = JSON.parse(string);\n      let id = deleteData.id;\n      try {\n        await axios({\n        method:\"delete\",\n        url:\"https://jsonplaceholder.typicode.com/posts/\"+ id,\n      });\n      this.loader= false;\n      this.response.splice(index,1);\n      }\n      catch(err){\n        this.loader= false;\n        alert('Something is not right');\n      }\n    },\n    edit(data){\n      this.save = true;\n      let string = JSON.stringify(data);\n      let editData = JSON.parse(string);\n      this.userId=editData.userId;\n      this.title=editData.title;\n      this.body=editData.body;\n    },\n    reset(){},\n    async update(){}\n  }\n}\n</script>\n\n<template>\n  <div class=\"container-fluid bg-light min-vh-100\">\n    <div class=\"row\">\n      <div class=\"col-md-3 bg-dark min-vh-100 p-4\">\n        <h4 class=\"text-light mb-4\">VueJS Post Request</h4>\n        <form @submit=\"(event) => save? update(event):post(event)\">\n            <input type=\"number\" placeholder=\"User Id\" class=\"form-control mb-3\" v-model=\"userId\" />\n            <input type=\"text\" placeholder=\"Title\" class=\"form-control mb-3\" v-model=\"title\" />\n            <textarea placeholder=\"Post\" class=\"form-control mb-3\" v-model=\"body\"></textarea>\n            <button type=\"submit\" class=\"btn btn-success\" v-if=\"!save\">CREATE</button>\n            <button type=\"submit\" class=\"btn btn-primary me-2\" v-if=\"save\">SAVE</button>\n            <button type=\"button\" class=\"btn btn-warning\" v-if=\"save\" @click=\"reset\">RESET</button>\n        </form>\n      </div>\n      <div class=\"col-md-9 p-4 overflow-hidden\">\n        <div class=\"card shadow-sm mb-4 animate__animated animate__backInRight animate__faster\" v-for=\"(data,index) in response\" :key=\"index\">\n          <div class=\"card-header d-flex justify-content-between\"> \n            {{ data.title }}\n            <div class=\"d-flex align-items-center\">\n              <button class=\"btn\" @click=\"edit(data,index)\"><i class=\"fa fa-edit text-info\"></i></button>\n              <button class=\"btn\" @click=\"deleteClick(data,index)\"><i class=\"fa fa-trash text-danger\"></i></button>\n            </div>\n          </div>\n          <div class=\"card-body\"> {{ data.body }}</div>\n        </div>\n        <i v-if=\"loader\" class=\"fa fa-spinner fa-spin\" style=\"font-size:40px\"></i>\n      </div>\n    </div>\n  </div>\n</template>"],"mappings":";AACA,OAAO,sCAAsC;AAC7C,OAAO,uCAAuC;AAC9C,OAAO,aAAa;AACpB,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,IAAI,EAAE,KAAK;MACXC,MAAM,EAAC,EAAE;MACTC,KAAK,EAAC,EAAE;MACRC,IAAI,EAAC,EAAE;MACPC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,IAAIA,CAACC,CAAC,EAAC;MACXA,CAAC,CAACC,cAAc,EAAE;MAClB,IAAI,CAACJ,MAAK,GAAI,I